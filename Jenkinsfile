  agent any
  stages {
    stage("Create Docker Container") {
      steps {
        echo "building container..."
      }
    }
    stage("Deploying System API") {
      steps {
        echo "sAPI deploying..."
      }
    }
    stage("Deploying Process API") {
      steps {
        echo "pAPI deploying..."
      }
    }
    stage("Deploying Experience API") {
      steps {
        echo "eAPI deploying..."
      }
    }
  }
}
